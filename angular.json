{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "lifeworks": {
      "root": "apps/lifeworks/",
      "sourceRoot": "apps/lifeworks/src",
      "projectType": "application",
      "prefix": "lw",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/lifeworks",
            "index": "apps/lifeworks/src/index.html",
            "main": "apps/lifeworks/src/main.ts",
            "polyfills": "apps/lifeworks/src/polyfills.ts",
            "tsConfig": "apps/lifeworks/tsconfig.app.json",
            "assets": [
              "apps/lifeworks/src/favicon.ico",
              "apps/lifeworks/src/assets",
              "apps/lifeworks/src/config"
            ],
            "styles": [
              "libs/styles/_styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/lifeworks/src/environments/environment.ts",
                  "with": "apps/lifeworks/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "lifeworks:build",
            "ssl": true,
            "sslCert": "local/server.crt",
            "sslKey": "local/server.key"
          },
          "configurations": {
            "production": {
              "browserTarget": "lifeworks:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "lifeworks:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/lifeworks/src/test.ts",
            "polyfills": "apps/lifeworks/src/polyfills.ts",
            "tsConfig": "apps/lifeworks/tsconfig.spec.json",
            "karmaConfig": "apps/lifeworks/karma.conf.js",
            "styles": [
              "apps/lifeworks/src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "apps/lifeworks/src/favicon.ico",
              "apps/lifeworks/src/assets",
              "apps/lifeworks/src/config"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/lifeworks/tsconfig.app.json",
              "apps/lifeworks/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "lifeworks-e2e": {
      "root": "apps/lifeworks-e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
