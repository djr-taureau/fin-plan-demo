{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach",
      "port": 9229
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach by Process ID",
      "processId": "${command:PickProcess}",
      "protocol": "inspector",
      "port": 5858,
      "sourceMaps": true,
    },
    {
      "name": "Debug Microservices",
      "type": "node",
      "request": "attach",
      "protocol": "inspector",
      "port": 5858,
      "sourceMaps": true,
      "preLaunchTask": "runFunctionsHost",
      "timeout": 25000
    }
  ]
}