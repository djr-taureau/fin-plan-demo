  "version": "2.0.0",
  "tasks": [{
    "label": "ms:debug:activity-log",
    "command": "func",
    "args": [
      "start",
      "--debug VSCode"
    ],
    "options": {
      "cwd": "${workspaceFolder}/dist/microservices/activity-log"
    },
    "dependsOn": [
      "npm: ms:build"
    ],
    "type": "shell",
    "presentation": {
      "echo": true,
      "reveal": "always",
      "focus": true,
      "panel": "dedicated",
      "showReuseMessage": true
    }
  },
  {
    "label": "ms:debug:notifications",
    "command": "func",
    "args": [
      "start",
      "--debug VSCode"
    ],
    "options": {
      "cwd": "${workspaceFolder}/dist/microservices/notifications"
    },
    "dependsOn": [
      "npm: ms:build"
    ],
    "type": "shell",
    "presentation": {
      "echo": true,
      "reveal": "always",
      "focus": true,
      "panel": "dedicated",
      "showReuseMessage": true
    }
  }]
}
